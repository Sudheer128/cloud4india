{
  "name": "cloud4india-website",
  "version": "1.0.0",
  "description": "Cloud4India - Exact AWS Website Clone",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "serve -s dist",
    "docker:build": "docker build -f Dockerfile.frontend -t cloud4india-frontend .",
    "docker:build:cms": "docker build -f Dockerfile.cms -t cloud4india-cms .",
    "docker:run": "docker run -d --name cloud4india-frontend -p 4001:80 cloud4india-frontend",
    "docker:stop": "docker stop cloud4india-frontend cloud4india-cms && docker rm cloud4india-frontend cloud4india-cms",
    "deploy:local": "docker-compose -f docker-compose.local.yml up --build -d",
    "deploy:server": "docker-compose up --build -d",
    "deploy:stop": "docker-compose down",
    "deploy:stop:local": "docker-compose -f docker-compose.local.yml down",
    "logs": "docker-compose logs -f",
    "logs:frontend": "docker-compose logs -f cloud4india-frontend",
    "logs:cms": "docker-compose logs -f cloud4india-cms",
    "health": "curl http://localhost:4002/api/health"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.16",
    "axios": "^1.12.2",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "serve": "^14.2.0",
    "tailwindcss": "^3.2.7",
    "vite": "^4.1.0"
  },
  "keywords": [
    "react",
    "aws",
    "cloud",
    "clone",
    "cloud4india"
  ],
  "author": "Cloud4India",
  "license": "MIT"
}
